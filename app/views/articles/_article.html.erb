<div id="<%= dom_id article %>">
  <p>
    <strong>Title:</strong>
    <%= article.title %>
  </p>

  <p>
    <strong>Content:</strong>
    <span  data-controller="typed" data-typed-strings-value="<%= [article.content] %>">
    </span>
  </p>

  <p>
    <strong>Location</strong>
    <%= article.location %>
  </p>

</div>
